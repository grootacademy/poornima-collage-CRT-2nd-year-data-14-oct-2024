package com.grootacademy.methodex;

public class Test {
	public static void main(String[] args) {
		MyUtils myUtils=new MyUtils();
		myUtils.saySomethig();
		myUtils.saySomethig();
		
		myUtils.saySomethig();
		myUtils.saySomethig();
		myUtils.saySomethig();
	}
}
class MyUtils{
	public void saySomethig() {
		System.out.println("Hello");
	}
}