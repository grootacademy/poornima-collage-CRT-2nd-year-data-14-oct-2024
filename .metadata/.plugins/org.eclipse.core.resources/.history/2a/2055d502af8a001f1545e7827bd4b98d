package com.grootacademy.methodex;

import java.util.Scanner;

public class Test {
	public static void main(String[] args) {
		MyUtils myUtils=new MyUtils();
		myUtils.saySomethig();
		myUtils.saySomethig();
		
		myUtils.saySomethig();
		myUtils.saySomethig();
		myUtils.saySomethig();
	}
}
class MyUtils{
	public void saySomethig() {
		System.out.println("Hello");
	}
	public void getFact() {
		Scanner sc=new Scanner(System.in);
	}
}