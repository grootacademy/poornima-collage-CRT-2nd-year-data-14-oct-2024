package com.grootsoftware.inex;

public class Test {
	public static void main(String[] args) {
		Dog d=new Dog();
		d.eat();d.bark();
	}
}
class Animal{
	String color="black";
	public void eat() {
		System.out.println("Animal is eating");
	}
}
class Dog extends Animal{
	public void bark() {
		System.out.println("Kuta bhonk rha hai");
	}
}