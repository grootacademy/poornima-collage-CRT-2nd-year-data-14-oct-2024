import javax.swing.JFrame;

public class MyFrame2 extends  JFrame{
	public MyFrame2() {
		setTitle("Event handling");
		Button b=new Button("Click me for Second Frame");
		b.setBounds(100, 100, 200, 30);
		setLayout(null);
		setVisible(true);
		setSize(500,500);
	}
}
