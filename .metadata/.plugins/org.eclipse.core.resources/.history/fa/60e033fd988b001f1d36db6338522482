package com.groot.awtex;

import java.awt.*;

public class Test {
	public static void main(String[] args) {
		Frame f=new Frame("My First Frame");
		 Label nameLabel=new Label("Name");
		 nameLabel.setBounds( 100, 50, 100, 30);
		 f.add(nameLabel);
	
		f.setLayout(null);
		f.setVisible(true);
		f.setSize(500, 500);
	}
}
